{
  "watch": {
    "stylelint-dev": {
      "patterns": [
        "/src/**/*.scss"
      ],
      "extensions": "scss"
    },
    "eslint": {
      "patterns": [
        "/src/**/*.js"
      ],
      "extensions": "js"
    }
  },
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.5.2",
    "@material-ui/icons": "^4.5.1",
    "axios": "^0.19.0",
    "materialize-css": "^1.0.0-rc.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-facebook-login": "^4.1.1",
    "react-github-login": "^1.0.3",
    "react-google-login": "^5.0.7",
    "react-rater": "^5.1.1",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-sass-js start-js",
    "build": "npm run eslint && npm run stylelint-build && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "eslint": "eslint --fix 'src/**/*.js'",
    "prettier": "prettier --write 'src/**/*.js'",
    "stylelint-dev": "stylelint 'src/**/*.scss' --cache --cache-location './.stylelintcache' ; exit 0",
    "stylelint-build": "stylelint 'src/**/*.scss'",
    "stylelint-fix-sass": "stylelint --fix 'src/**/*.scss'",
    "watch-sass-js": "npm-watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run build",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.6.0",
    "prettier": "^1.19.1",
    "standart": "^6.1.0",
    "stylelint": "^11.1.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^3.1.1"
  }
}
